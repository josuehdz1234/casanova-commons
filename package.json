{
  "name": "@casanova/casanova-common",
  "version": "0.0.1",
  "description": "",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "scripts": {
    "build:js": "babel src --out-dir lib --copy-files",
    "build:css": "rollup -c",
    "build:icons": "svgr --no-dimensions assets/svg -d src/Icons/ --svg-props height=100%",
    "dev:js": "babel src --out-dir lib --watch --copy-files",
    "dev:css": "rollup -c -w",
    "build": "npm run build:icons && concurrently \"npm run build:css\" \"npm run build:js\"  ",
    "start": "concurrently \"npm run dev:js\" \"npm run dev:css\"",
    "test": "jest src",
    "coverage": "jest --ci --coverage --no-watch src",
    "postinstall": "npm run build",
    "sonarqube": "sonar-scanner",
    "eslint": "eslint src --fix"
  },
